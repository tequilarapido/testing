{
    "name": "tequilarapido/testing",
    "description": "Testing helpers.",
    "homepage": "https://github.com/tequilarapido/testing",
    "license": "MIT",
    "authors": [
        {
            "name": "Nassif Bourguig",
            "email": "nbourguig@gmail.com",
            "homepage": "https://github.com/nbourguig",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "5.1.* || 5.2.* || 5.3.*"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*",
        "orchestra/testbench": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Tequilarapido\\Testing\\": "src"
        }, 
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tequilarapido\\Testing\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
