{
    "name": "tequilarapido/testing",
    "description": "Testing helpers.",
    "homepage": "https://github.com/tequilarapido/testing",
    "license": "MIT",
    "authors": [
        {
            "name": "Nassif Bourguig",
            "email": "nbourguig@gmail.com",
            "homepage": "https://github.com/nbourguig",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "5.8.*",
        "laravel/browser-kit-testing": "5.1.*",
        "symfony/css-selector": "^4.2",
        "symfony/dom-crawler": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Tequilarapido\\Testing\\": "src"
        }, 
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tequilarapido\\Testing\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
